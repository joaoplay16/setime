<?xml version="1.0" encoding="utf-8" ?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:gravity="center_vertical"
	android:layout_width="match_parent"
	android:layout_height="wrap_content"
	android:padding="5dp">

	<TextView
		android:id="@+id/txtNome"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_alignParentStart="true"
		android:layout_alignParentTop="true"
		android:fontFamily="sans-serif-condensed"
		android:text="Nome"
		android:textAppearance="?android:textAppearanceMedium" />

	<TextView
		android:id="@+id/txtProcedimento"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_below="@+id/txtNome"
		android:layout_alignParentStart="true"
		android:fontFamily="sans-serif-black"
		android:text="Procedimento" />

	<TextView
		android:id="@+id/txtDiaDaSemana"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_below="@id/txtProcedimento"
		android:layout_alignParentStart="true"
		android:fontFamily="@font/allerta"
		android:text="Dia"
		android:layout_alignBottom="@id/txtData"
		android:textSize="14sp" />
	<TextView android:id="@+id/txtData"
		android:text="Data"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginLeft="5dp"
		android:fontFamily="@font/aldrich"
		android:layout_below="@id/txtProcedimento"
		android:layout_toEndOf="@id/txtDiaDaSemana" />

	<TextView
		android:id="@+id/txtHora"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_alignTop="@id/txtData"
		android:layout_marginLeft="5dp"
		android:layout_toRightOf="@id/txtData"
		android:fontFamily="@font/aldrich"
		android:text="Hora" />
	<TextView android:id="@+id/txtValor"
		android:text="Valor"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_alignParentEnd="true"
		android:layout_alignTop="@id/txtData"
		android:fontFamily="@font/allerta"



		/>
</RelativeLayout>
